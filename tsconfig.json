{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "wwwroot/js/compiled",
    "moduleResolution": "node",
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "types": [ "jquery" ]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}